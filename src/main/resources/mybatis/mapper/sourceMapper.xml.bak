<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.laowang.bsbdj.mapper.SourceMapper">
    <insert id="insert" parameterType="com.laowang.bsbdj.entity.Source" >
       insert into t_source(channel_id,response_text,create_time,url,state)
       values (#{channelId},#{responseText},#{createTime},#{url},#{state});
      </insert>
</mapper>